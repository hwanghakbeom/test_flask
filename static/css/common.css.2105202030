.btn_round{position:relative; display:block; box-sizing:border-box; width:100%; height:50px; line-height:48px; border:1px solid #ff8a00; border-radius:42px; background:#fff; cursor:pointer; text-align:center; color:#ff8a00; font-size:17px; font-weight:700; text-decoration:none}
.btn_round_h60{height:60px; line-height:58px; font-size:18px}
.btn_square{position:relative; display:inline-block; box-sizing:border-box; width:100%; height:50px; line-height:48px; border:1px solid #ff8a00; border-radius:5px; background:#fff; cursor:pointer; text-align:center; color:#ff8a00; font-weight:700; text-decoration:none}
