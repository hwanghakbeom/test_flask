@import url(/views/assets/css/nanumgothic.css);

*{font-family:Nanum Gothic; font-size:13px}

html{height:100%}
body{margin:0; padding:0; min-height:420px; background:#fff}
ol, ul, li{list-style:none;vertical-align:top;margin:0;padding:0;line-height:1.5em}
img{border:0 none}
p{margin:0 0 12px 0; line-height:1.5em}
dl, dd{margin:2px 0; line-height:1.5em}
a:link, a:active, a:visited{text-decoration:none}
a:hover{text-decoration:none}
hr{display:none}
h1{margin:0; padding-top:19px}
h2{font-size:37px; color:#ff8a00; text-align:center}
h3{position:relative; font-size:15px; margin:40px 0 5px 0}
input[type=text], input[type=password], input[type=tel], select{height:28px; border:0; outline:0; background:#fff; box-sizing:border-box; padding-left:10px}
input[type=checkbox], input[type=radio]{position:relative; top:1.5px; margin:0}
ul.disc li{list-style:disc; margin:0 0 0 20px; padding:2px 0; line-height:1.5em}
select:invalid {color:#767676}

#header{margin:0 20px; height:69px; border-bottom:1px solid #ccc}
#container{margin:0 20px 80px 20px;}
#content{position:relative; min-height:450px; padding:0}
