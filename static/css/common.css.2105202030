.uline{padding:0 0 10px 0; border-bottom:1px solid #ccc}
.hide{display:none}
.input_val dt{padding-top:15px; color:#333; font-weight:700}
.input_val dt, .input_val dd{line-height:32px; clear:both}
.input_val dd{position:relative; height:54px; padding:4px 0; border:1px solid #c5c5c5; border-radius:5px; box-sizing:border-box}
.input_val dd.no_line{border:none;}
.input_val dd.right{text-align:right}
.input_val dd.readonly, .input_val dd.readonly input{background:#f0f0f0}
.input_val dd.h100, .input_val dd.h100 textarea{height:100px}
.input_val input[type=text], .input_val input[type=password], .input_val input[type=tel],
.input_val select{width:100%; height:100%; padding:0 10px}
.input_val select{width:-webkit-calc(100% - 5px)!important; width:-moz-calc(100% - 5px)!important; width:calc(100% - 5px)!important}
.input_val textarea{width:100%; height:54px; margin-right:4px; padding:10px; outline:0; border:1px solid #c5c5c5; border-radius:5px; box-sizing:border-box}
.input_val dd.email input, .input_val dd.email select{width:45%!important}
.input_val dd.rpad4btn input[type=text], .input_val dd.rpad4btn input[type=password], .input_val dd.rpad4btn input[type=tel]{width:-webkit-calc(100% - 88px)!important; width:-moz-calc(100% - 88px)!important; width:calc(100% - 88px)!important}
.input_val dd.rpad4btn2 input[type=text], .input_val dd.rpad4btn2 input[type=password], .input_val dd.rpad4btn2 input[type=tel]{width:-webkit-calc(100% - 60px)!important; width:-moz-calc(100% - 60px)!important; width:calc(100% - 60px)!important}
.input_val dd.rpad4btn3 input[type=text], .input_val dd.rpad4btn3 input[type=password], .rpad4btn3 dd.rpad4btn2 input[type=tel]{width:-webkit-calc(100% - 40px)!important; width:-moz-calc(100% - 40px)!important; width:calc(100% - 40px)!important}
.input_val dd .btn_square{width:80px; height:44px!important; line-height:42px}
/*.input_val dd input.price{padding-right:40px}*/
.input_val dd span.won{position:absolute; right:10px; top:10px; font-size:15px; font-weight:700}
.input_val .info_txt{color:#ff8a00}

.input_val.select{margin:30px 0}
.input_val.select li input{width:100%; height:100%; padding:0}
.input_val.select li{position:relative; cursor:pointer; height:54px;margin:10px 0; line-height:46px; padding:4px 0 4px 45px; font-size:16px; border:1px solid #c5c5c5; border-radius:5px; background:url('/views/assets/images/ico_select_off.png') no-repeat 20px 21px; box-sizing:border-box}
.input_val.select li.on{background:url('/views/assets/images/ico_select_on.png') no-repeat 20px 21px}
.input_val.select li.btn{background:none; padding:4px 0; text-align:center; color:#999}
.input_val.select li.rpad4btn2 input[type=text], .input_val.select li.rpad4btn2 input[type=password], .input_val.select li.rpad4btn2 input[type=tel]{width:-webkit-calc(100% - 60px)!important; width:-moz-calc(100% - 60px)!important; width:calc(100% - 60px)!important}
.input_val.draggable li{background:url('/views/assets/images/ico_draggable.png') no-repeat 20px 21px}
.input_val.draggable li.btn{background:none}
.input_val.draggable li .btn_ico.write{position:absolute; right:50px; top:15px}
.input_val.draggable li .btn_ico.delete{position:absolute; right:10px; top:15px}

.member h2{margin:100px 0 30px 0}
