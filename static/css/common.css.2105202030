.btn_search{position:absolute; display:inline-block; box-sizing:border-box; top:11px; right:5px; width:30px; height:30px; border:none; color:transparent; background:url('/views/assets/images/ico_search.png') no-repeat 5px 4px;  cursor:pointer; vertical-align:middle}
.btn_scan{position:absolute; display:inline-block; box-sizing:border-box; top:11px; right:50px; width:30px; height:30px; border:none; color:transparent; background:url('/views/assets/images/ico_scan.png') no-repeat 5px 4px;  cursor:pointer; vertical-align:middle}
.btn_tab{position:relative; display:inline-block; box-sizing:border-box; height:30px; line-height:28px; padding:0 15px; border-radius:5px 5px 0 0; background:#fff; cursor:pointer; text-align:center; color:#ff8a00; font-weight:700; text-decoration:none}
.btn_tab.btn_c_gray{border:1px solid #4d4d4d; border-bottom:none; background:#a3a3a3; color:#fff}
